        "id": "@id",
        "@protected": true,
        "@version": 1.1
      }
    },
    "ChildDetails": {
      "@id": "schema:ChildDetails",
      "@type": "@id",
      "@context": {
        "id": "@id",
        "fullName": "schema:name",
        "dateOfBirth": "schema:birthDate",
        "sex": "schema:gender",
        "placeOfBirth": "schema:birthPlace",
        "nationality": "schema:nationality"
      }
    },
    "MotherDetails": {
      "@id": "schema:MotherDetails",
      "@context": {
        "id": "@id",
        "fullName": "schema:name",
        "dateOfBirth": "schema:birthDate",
        "nationality": "schema:nationality"
      }
    },
    "FatherDetails": {
      "@id": "schema:FatherDetails",
      "@context": {
        "id": "@id",
        "fullName": "schema:name",
        "dateOfBirth": "schema:birthDate",
        "nationality": "schema:nationality"
      }
    },
    "RegistrationDetails": {
      "@id": "schema:RegistrationDetails",
      "@context": {
        "id": "@id",
        "registrationNumber": "schema:identifier",
        "dateOfRegistration": "schema:startDate",
        "dateOfIssuance": "schema:startDate"
      }
    }
