      "@protected": false
    },
    "type": [
            "VerifiableCredential",
            "HealthInsuranceCredential",
            "LifeInsuranceCredential"
        ],
    "schema": "https://schema.org/",
    "InsuranceCredential": {
      "@id": "https://raw.githubusercontent.com/challabeehyv/mimoto-config/main/insurance-context.json#InsuranceCredential"
